@{
    ViewBag.Title = "Home Page";
}

<div class="container">
    <br> <br> <br>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">

        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="img"></li>
            <li data-target="#myCarousel" data-slide-to="1" class="item"</li>
            <li data-target="#myCarousel" data-slide-to="2" class="item"></li>
            <li data-target="#myCarousel" data-slide-to="3" class="item"></li>
            <li data-target="#myCarousel" data-slide-to="4" class="item"></li>
            <li data-target="#myCarousel" data-slide-to="5" class="item"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="img">
                <img width="1139" height="449" alt="" src="img/img1.jpg" />
            </div>

            <div class="item">
                <img width="1139" height="449" alt="" src="img/img3.jpg" />
            </div>

            <div class="item">
                <img width="1139" height="449" alt="" src="img/img10.jpg" />
            </div>

            <div class="item">
                <img width="1139" height="449" alt="" src="img/img11.jpg" />
            </div>

            <div class="item">
                <img width="1139" height="449" alt="" src="img/img12.jpg" />
            </div>

            <div class="item">
                <img width="1139" height="449" alt="" src="img/img13.jpg" />
            </div>
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
